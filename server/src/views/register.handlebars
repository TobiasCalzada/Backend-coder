<h1>Register Form</h1>
<section class=" d-flex m-5">
    <form class="d-flex flex-column p-5 d-flex justify-content-center align-items-center">
        <input class="mt-2" type="text" name="name" id="name" placeholder="name">
        <input class="mt-2" type="email" name="email" id="email" placeholder="email">
        <input class="mt-2" type="password" name="password" id="password" placeholder="password">
        <input class="mt-2" type="text" name="photo" id="photo" placeholder="photo">
        <button type="button" id="register" value="REGISTER"  class="btn btn-warning  mt-2">Register</button>
    </form>
    <article class="fs-6 pt-5">
        lastest users
        <span id="update" ></span>
    </article>
</section>